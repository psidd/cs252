<table class="engineTable" id="shading-desc">
 <tr class="data2" style="background-color: #ccffcc">
  <td class="left">
   By default, the shading indicates records from the past week, or failing that, the most recent entry.
  </td>
 </tr>
 <tr class="data2" style="background-color: #ccffcc">
  <td class="left">
   <b>Adjust:</b>
   <a id="shade_recent" title="most recent records" href="#" onclick="daysShading(0, 1);return false;">most recent</a>
   | <a id="shade_7" title="records from the past week" href="#" onclick="daysShading(7);return false;">past week</a>
   | <a id="shade_31" title="records from the past month" href="#" onclick="daysShading(31);return false;">past month</a>
   | <a title="records from the past six months" href="#" onclick="daysShading(181);return false;">past 6 months</a>
   | <a title="records from the past year" href="#" onclick="daysShading(365);return false;">past year</a>
   | <a title="records from the past four years" href="#" onclick="daysShading(1461);return false;">4 years</a>
   | <a title="records from the past ten years" href="#" onclick="daysShading(3652);return false;">10 years</a>
   | <a title="records from the past 25 years" href="#" onclick="daysShading(9131);return false;">25 years</a>
  </td>
 </tr>
</table>

<script type="text/javascript" language="javascript">
function daysShading(days, show_recent) {
	var ts = document.getElementsByClassName("engineTable");
	var i, j, dd_exists, dd_week;
	var dd_max = 0;
	for (i = 0; i < ts.length; i++) {
		var trs = ts[i].getElementsByTagName("tr");
		for (j = 0; j < trs.length; j++) {
			var dd = trs[j].getAttribute("data-days");
			if (dd) {
				dd_exists = 1;
				if (737649 - dd < days) {
					trs[j].style.backgroundColor = "#ccffcc";
					dd_week++;
				}
				else {
					trs[j].style.backgroundColor = null;
				}
				if (dd > dd_max)
					dd_max = dd;
			}
		}
	}

	if (!dd_week && dd_max && show_recent) {
		for (i = 0; i < ts.length; i++) {
			var trs = ts[i].getElementsByTagName("tr");
			for (j = 0; j < trs.length; j++) {
				var dd = trs[j].getAttribute("data-days");
				if (dd) {
					if (dd == dd_max)
						trs[j].style.backgroundColor = "#ccffcc";
					else
						trs[j].style.backgroundColor = null;
				}
			}
		}
	}
	
	if (!dd_exists)
		document.getElementById('shading-desc').style.display = 'none';
}

daysShading(7, 1);

</script>
<table class="engineTable" style="margin:0px;">
 <tr class="data2">
